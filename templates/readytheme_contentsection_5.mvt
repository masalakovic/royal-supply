				<mvt:do file="g.Module_Feature_URI_UT" name="l.settings:readytheme:featured_category_02:uri" value="Store_Product_URL(l.settings:rt_product, NULL)" />
			<mvt:elseif expr="l.settings:readytheme:featured_category_02:link_type EQ 'U'">
				<mvt:assign name="l.settings:readytheme:featured_category_02:uri" value="l.settings:readytheme:featured_category_02:link_dest" />
			<mvt:else>
				<mvt:assign name="l.settings:readytheme:featured_category_02:uri" value="NULL" />
			</mvt:if>
			<a href="&mvt:readytheme:featured_category_02:uri;" target="&mvte:readytheme:featured_category_02:link_targ;"></a>
		</mvt:if>
	</figure>
</section>
