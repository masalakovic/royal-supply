				<mvt:do file="g.Module_Feature_URI_UT" name="l.settings:readytheme:featured_category_02:uri" value="Store_Page_URL(l.settings:rt_page, NULL)" />
			<mvt:elseif expr="l.settings:readytheme:featured_category_02:link_type EQ 'C'">
