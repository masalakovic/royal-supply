<mvt:if expr="NOT l.settings:mmx:loaded:base">
	<mvt:item name="head" param="css:mmx-base" />
	<mvt:item name="head" param="js:mmx-base" />
	<mvt:assign name="l.settings:mmx:loaded:base" value="1" />
</mvt:if>

<mvt:if expr="NOT l.settings:mmx:loaded:text">
