	<mvt:assign name="l.settings:mmx:loaded:text" value="1" />
</mvt:if>

<mvt:if expr="NOT l.settings:mmx:loaded:text_accordion">
	<mvt:item name="head" param="css:mmx-text-accordion" />
	<mvt:item name="head" param="js:mmx-text-accordion" />
	<mvt:assign name="l.settings:mmx:loaded:text_accordion" value="1" />
</mvt:if>
